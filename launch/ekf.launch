<launch>
  <node pkg="docking_control" type="odometry_remap.py" name="remap_marker_to_odometry" />

  <node pkg="robot_localization" type="ekf_localization_node" name="robot_pose_ekf">
    <param name="world_frame" value="map" />
      <param name="odom_frame" value="odom" />
    <param name="imu0" value="mavros/imu/data"/>
    <rosparam param="imu0_config">[false,  false,  false,
                                true, true, true,
                                false,  false, false,
                                true, true, true,
                                true, false, false]</rosparam>
  <param name="imu0_remove_gravitational_acceleration" value="true" />
    <param name="pose0" value="boat/pose"/>
    <rosparam param="pose0_config">[true,  true,  true,
                                false, false, false,
                                false,  false, false,
                                false, false, false,
                                false, false, false]</rosparam>

  </node>

</launch>